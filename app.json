{
  "expo": {
    "name": "IOTApp",
    "slug": "IOTApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "@react-native-voice/voice"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.IOTApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.IOTApp"
    },
    "web": {
      "favicon": "./assets/logoo.png"
    },
    "extra": {
      "eas": {
        "projectId": "6c4c58a5-fadb-44b8-a5de-43073f94d304"
      }
    }
  }
}
